{
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "name": "tree-sitter-snakemake",
  "version": "0.0.1",
  "description": "snakemake grammar for tree-sitter, based on tree-sitter-python",
  "main": "bindings/node",
  "scripts": {
    "test": "(cd tree-sitter-snakemake && tree-sitter test)",
    "build": "(cd tree-sitter-snakemake && tree-sitter generate --no-bindings)"
  },
  "keywords": [
    "parser",
    "snakemake",
    "python"
  ],
  "author": "Oliver Thomas",
  "license": "MIT",
  "tree-sitter": [
    {
      "scope": "source.snakemake",
      "file-types": [
        "Snakefile",
        "smk"
      ]
    }
  ]
}
